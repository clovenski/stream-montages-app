{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "Stream Montages App - API Gateway",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "debug_endpoint": true,
  "endpoints": [
    {
      "endpoint": "/montages/jobs",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/montages/jobs",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "http://jobs-repo-service:8080"
          ],
          "disable_host_sanitize": false,
          "is_collection": true,
          "target": "",
          "group": "",
          "mapping": {
            "collection": "items"
          }
        }
      ]
    },
    {
      "endpoint": "/montages/jobs",
      "method": "POST",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/montages/jobs",
          "encoding": "json",
          "sd": "static",
          "method": "POST",
          "host": [
            "http://jobs-repo-service:8080"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
