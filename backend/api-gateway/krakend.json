        {
          "url_pattern": "/montages/jobs/{id}",
          "encoding": "string",
          "sd": "static",
          "method": "DELETE",
          "host": [
            "http://jobs-repo-service:8080"
          ],
          "disable_host_sanitize": false
        }
      ]
