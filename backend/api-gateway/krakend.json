        {
          "url_pattern": "/montages",
          "encoding": "json",
          "sd": "static",
          "method": "POST",
          "host": [
            "http://montages-repo-service:8080"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/montages/{id}",
      "method": "GET",
